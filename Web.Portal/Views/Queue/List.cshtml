@{
    Layout = null;
    int PageCurrent = 0;

}

<div class='col-md-2 col-xs-12 margin-top-10'>
    <label class='control-label'><b>Tổng số: @ViewBag.TotalRecord</b></label>
</div>
<table class="table table-striped table-bordered table-advance table-hover dt-responsive" id="tbl-invoice">
    <thead>
        <tr>
            <th class="all" style="width:3%"></th>
            <th style="width:10%">
                SỐ PHIẾU XUẤT KHO
            </th>
            <th style="width:10%">
                MAWB
            </th>
            <th style="width:15%">
                HAWB
            </th>
            <th style="width:10%">
               NGÀY TẠO
            </th>
            <th style="width:5%">
              NHÂN VIÊN
            </th>

            <th style="width:10%">
                CẤP SỐ
            </th>
            <th style="width:10%">
                NGÀY CẤP SỐ
            </th>
           

        </tr>
    </thead>
    <tbody>
        @foreach (var row in (IList<Web.Portal.Layer.QueueAWB>)ViewData["QueueLists"])
            {
                PageCurrent += 1;

            <tr>
                <td>@PageCurrent</td>
                <td><a href="#" >@Html.DisplayTextFor(model => row.INVOICENO)</a></td>
                <td><a href="#" >@Html.DisplayTextFor(model => row.MAWB)</a></td>
                <td><a href="#">@Html.DisplayTextFor(model => row.HAWB)</a></td>
                <td><a href="#" >@Html.Raw(row.INVOICECREATED.HasValue?row.INVOICECREATED.Value.ToString("dd/MM/yyyy HH:mm:ss"):string.Empty)</a></td>
                <td><a href="#" >@Html.DisplayTextFor(model => row.EMPLOYEE)</a></td>
                <td><a href="#">@Html.DisplayTextFor(model => row.QUEUE)</a></td>
                <td><a href="#">@Html.Raw(row.QUEUECREATED.HasValue ? row.QUEUECREATED.Value.ToString("dd/MM/yyyy HH:mm:ss") : string.Empty)</a></td>
            </tr>
        }
    </tbody>

</table>
<script>
        egovutil.CreateTableSort("#tbl-invoice", [[0, "asc"]], [

        {

            "orderable": false, "targets": [0, 0]
        }
        ], false);
</script>
