@{
    Layout = null;
    int PageCurrent = 0;
}

<div style="width:100%;overflow-x:auto">
    <table class="table table-striped table-bordered table-advance table-hover dt-responsive" id="tbl-hawb">
        <thead>
            <tr>
                <th class="all" style="width:3%"></th>
                <th style="width:15%">NAME</th>
                <th style="width:15%">
                    GROSS WEIGHT
                </th>
                <th style="width:15%">
                    TARE WEIGHT
                </th>
                <th style="width:15%">
                    NET WEIGHT
                </th>
                <th style="width:15%">
                    UNLOADING POINTS
                </th>
                <th style="width:15%">
                    REMARK
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var row in (IList<Web.Portal.Common.ViewModel.ContViewModel>)ViewData["contList"])
            {
                PageCurrent += 1;

                <tr>
                    <td>@PageCurrent</td>
                <td align="right">@Html.DisplayTextFor(model => row.Name)</td>
                <td align="right">@Html.DisplayTextFor(model => row.GrossWeight)</td>
                <td align="right">@Html.DisplayTextFor(model => row.TareWeight)</td>
                <td align="right">@Html.DisplayTextFor(model => row.NetWeight)</td>
                <td align="right">@Html.DisplayTextFor(model => row.Unloading)</td>
                <td align="right">@Html.DisplayTextFor(model => row.Remark)</td>
              
            </tr>
            }
        
        </tbody>

    </table>
</div>
<script>
   
</script>