
@{
    Layout = null;
    int PageCurrent = 0;
    string[] columns = (string[])ViewData["column"];
    System.Data.DataTable tableReport = ((System.Data.DataTable)ViewData["DataList"]);

}

<div class='col-md-2 col-xs-12 margin-top-10'>
    <label class='control-label'><b>Tổng số: @ViewBag.TotalRecord</b></label>
</div>
<table class="table table-striped table-bordered table-advance table-hover dt-responsive" id="tbl-report">

    <thead>
        <tr>
            <th class="all" style="width:3%;font-size:16px;background-color:yellow !important;text-align:center">STT</th>
            <th style="width:10%;font-size:14px;background-color:yellow !important;text-align:center">
                Ngày
            </th>
            <th style="width:10%;font-size:14px;background-color:yellow !important;text-align:center">
                Số AWB
            </th>
          
            <th style="width:10%;font-size:14px;background-color:yellow !important;text-align:center">
                Weight
            </th>
            <th style="width:10%;font-size:14px;background-color:yellow !important;text-align:center">
                ChargeAble
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var row in (List<Web.Portal.Common.ViewModel.HandlingReportViewModel>)ViewData["listHandling"])
        {
            PageCurrent += 1;

            <tr>
                <td>@PageCurrent</td>
                <td><a href="#" class="btn  btn-xs sqs-member-edit">@Html.Raw(row.Date)</a></td>
                <td><a href="#" class="btn  btn-xs sqs-member-edit">@Html.Raw(row.AWB)</a></td>
               
                <td>
                    <a href="#" class="btn  btn-xs sqs-member-edit">@Html.Raw(row.Weight)</a>
                </td>
                <td>
                    <a href="#" class="btn  btn-xs sqs-member-edit">@Html.DisplayTextFor(model => row.ChargeableWeigt)</a>
                </td>



            </tr>
        }
    </tbody>

</table>
