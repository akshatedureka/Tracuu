<script>
    var message = {
        userName: {
            required: "Tên đăng nhập không được trống"
        },
        password: {
            required: "Mật khẩu không được trống"
        }
    };
    var rule = {
        userName: {

            required: true
        },
        password: {
            required: true
        }
    };
    egovutil.submitCallBack("#frmAction", rule, message, "/login/check?ReturnUrl=@Request["ReturnUrl"]");
</script>

